@model List<MenuItem>

<div class="navbar-nav">
    @foreach (var item in Model)
    {
        @if (item.isPage)
        {
            <a class="nav-item nav-link @item.Active"
               asp-area="@item.Area"
               asp-page="@item.Page">
                @item.Text
            </a>
        }
        else
        {
            <a class="nav-item nav-link @item.Active"
               asp-controller="@item.Controller"
               asp-action="@item.Action">
                @item.Text
            </a>
        }

    }

